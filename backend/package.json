{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "server": "pm2 start index.js --no-daemon",
    "start": "node index.js",
    "server:stop": "pm2 kill",
    "dev": "nodemon index.js",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo:all",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' ",
    "seed": "sequelize db:seed:all",
    "migrate:prod": "npx sequelize db:migrate --env production",
    "migrate:prod:undo": "npx sequelize db:migrate:undo:all --env production",
    "seed:prod": "npx sequelize db:seed:all  --env production",
    "seed:undo:prod": "npx sequelize db:seed:undo:all  --env production",
    "migrate:staging": "npx sequelize db:migrate --env staging",
    "seed:staging": "npx sequelize db:seed:all --env staging",
    "seed:undo": "sequelize db:seed:undo:all"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^8.0.2",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.9.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "pm2": "^5.3.0",
    "sequelize": "^6.32.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "morgan": "^1.10.0",
    "prettier": "^3.0.3",
    "sequelize-cli": "^6.6.1"
  }
}
